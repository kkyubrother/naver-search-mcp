{
  "name": "@isnow890/naver-search-mcp",
  "version": "1.0.30",
  "description": "Naver Search MCP Server",
  "main": "dist/index.cjs",
  "module": "./src/index.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npx shx rm -rf dist && esbuild src/index.ts --bundle --platform=node --format=cjs --target=node18 --outfile=dist/index.cjs",
    "prepare": "npm run build",
    "watch": "esbuild src/index.ts --bundle --platform=node --format=cjs --target=node18 --outfile=dist/index.cjs --watch",
    "start": "node dist/index.cjs"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "1.17.1",
    "axios": "^1.6.7",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/node": "^22",
    "esbuild": "^0.24.0",
    "shx": "^0.3.4",
    "typescript": "^5.3.3"
  },
  "author": "isnow890",
  "license": "MIT",
  "bin": {
    "naver-search-mcp": "dist/index.cjs"
  },
  "type": "module"
}
